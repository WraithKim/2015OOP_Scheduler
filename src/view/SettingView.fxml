<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import extfx.scene.control.RestrictiveTextField ?>

<HBox fx:controller="view.event.SettingControl" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="114.0" prefWidth="278.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Student ID">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <RestrictiveTextField fx:id="settingSIDInputForm" prefHeight="25.0" prefWidth="111.0" maxLength="8" restrict="[0-9]" onAction="#handleSaveAction" onKeyTyped="#handleChangedIDInputForm">
         <HBox.margin>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </RestrictiveTextField>
      <Button fx:id="settingSaveButton" mnemonicParsing="false" onAction="#handleSaveAction" text="Save">
         <HBox.margin>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
